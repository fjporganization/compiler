# program reads 2 fraction in loop and writes bigger of them
# ends when one of the readed fractions is zero

 void main(){
  ratio a;
  ratio b;
  ratio bigger;
  
  boolean aIsBigger;
  
  a = b = 1|1;
  
  do{
    {a, b} = {readfrac(), readfrac()};
    aIsBigger = a > b;
    bigger = aIsBigger ? a : b;
    writefrac(bigger);
    aIsBigger = true;
  }while(true);
 }